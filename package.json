{
    "name": "earthkingdoms-launcher",
    "productName": "EarthKingdoms Launcher",
    "version": "2.2.0",
    "description": "Launcher Minecraft pour le serveur EarthKingdoms",
    "main": "src/app.js",
    "author": {
        "name": "EarthKingdoms Inc.",
        "email": "mail.earthkingdoms@gmail.com",
        "url": "https://earthkingdoms-mc.fr"
    },
    "url": "https://earthkingdoms-mc.fr",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=dev electron .",
        "dev": "cross-env-shell DEV_TOOL=open nodemon --exec npm start",
        "build": "cross-env ELECTRON_BUILDER_ALLOW_HANDLE_SYMLINK_AS_FILE=true node build.js --obf=true --build=platform",
        "icon": "node build.js --icon=https://raw.githubusercontent.com/EarthKingdoms/EarthKingdoms-Launcher/master/src/assets/images/icon.png"
    },
    "license": "CCANC",
    "nodemonConfig": {
        "ext": "js,html,css",
        "ignore": "**/test"
    },
    "build": {
        "appId": "com.earthkingdoms.launcher",
        "productName": "EarthKingdoms Launcher"
    },
    "dependencies": {
        "electron-updater": "^6.6.2",
        "minecraft-java-core": "^4.2.3",
        "node-bdd": "^2.0.4",
        "node-fetch": "^2.7.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "cross-env": "^10.1.0",
        "electron": "32.0.0",
        "electron-builder": "^26.0.12",
        "javascript-obfuscator": "^4.1.1",
        "jimp": "^1.6.0",
        "nodemon": "^3.1.11",
        "png2icons": "^2.0.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EarthKingdoms/EarthKingdoms-Launcher.git"
    }
}